<%= form_tag protests_path, method: :get do %>
  <%= collection_select :search_station, params[:search_station], Station.all, :id, :name, prompt: "Select Station" %>
  <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>

<%= form_tag protests_path, method: :get do %>
  <%= collection_select :search_category, params[:search_category], AssaultCategory.all, :id, :name, prompt: "Select Assault Type" %>
  <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>

<%= form_tag protests_path, method: :get do %>
  <%= select_date Date.today, prefix: :start_date%>
  <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>
